{
  "name": "service",
  "version": "0.0.0",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "NODE_ENV=local ts-node ./src",
    "dev:hot": "NODE_ENV=local nodemon",
    "lint": "eslint --flag unstable_ts_config",
    "test": "NODE_ENV=test ts-node ./spec",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^9.1.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-validators": "^1.3.5",
    "jsend": "^1.1.0",
    "mysql2": "^3.14.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@swc/core": "^1.11.8",
    "@types/bcrypt": "^5.0.2",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.15.32",
    "@types/validator": "^13.15.2",
    "eslint": "^9.22.0",
    "eslint-plugin-n": "^17.16.2",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
